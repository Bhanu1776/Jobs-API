{
  "name": "rest-api",
  "version": "1.0.0",
  "description": "This is a RESTFUL API Built with Nodejs and Typescript",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon",
    "build": "rimraf ./build && tsc",
    "start2": "npm run build && node build/index.js",
    "start": "vercel dev",
    "deploy": "vercel deploy --prod",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add build"
  },
  "config": {
    "cz-emoji": {
      "skipQuestions": [
        "issues",
        "breaking"
      ]
    }
  },
    "pre-commit": [
      "ts.check",
      "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bhanu1776/REST-API.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Bhanu1776/REST-API/issues"
  },
  "homepage": "https://github.com/Bhanu1776/REST-API#readme",
  "devDependencies": {
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pre-commit": "^1.2.2",
    "vercel": "^28.13.2"
  }
}
